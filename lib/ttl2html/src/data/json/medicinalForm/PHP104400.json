    "xml": "<topic id=\"PHP104400\" outputclass=\"medicinalForm\" type=\"medicinalForm\" namespace=\"/drugs/calcium-polystyrene-sulfonate/powder\" basename=\"powder\" title=\"Powder\">\n<title>Powder</title>\n<body>\r\n<section outputclass=\"cautionaryAndAdvisoryLabels\">\r\n<title>Cautionary and advisory labels</title>\r\n<p>Label <ph outputclass=\"labelNumber\">13</ph> - Dissolve or mix with water before taking </p>\r\n<p>Label <ph outputclass=\"labelNumber\">21</ph> (Sorbisterit<tm tmtype=\"reg\"/> powder only) - Take with or just after food, or a meal </p>\n</section>\r\n<section outputclass=\"excipients\">\r\n<title>Excipients</title>\r\n<p outputclass=\"introductoryText\">May contain</p>\r\n<p>Sucrose</p>\n</section>\r\n\r\n<section outputclass=\"clinicalMedicinalProductInformationGroup\">\n<xref format=\"dita\" href=\"#PHP106986\" outputclass=\"clinicalMedicinalProductInformationXref\" namespace=\"/clinical-medicinal-product-information/sorbisterit-powder\">Sorbisterit<tm tmtype=\"reg\"/> powder</xref>\n</section>\r\n<section outputclass=\"medicinalProduct\">\r\n<title>Sorbisterit powder <ph outputclass=\"manufacturer\">(Stanningley Pharma Ltd)</ph>\n</title>\n<data name=\"ampid\">19210311000001101</data>\r\n<p outputclass=\"strengthOfActiveIngredient\">Calcium polystyrene sulfonate 854&#8239;mg per 1&#8239;gram</p>\r\n<ul outputclass=\"packs\">\r\n<li outputclass=\"packSizePriceTariff\">\r\n<p outputclass=\"packInfo\">\r\n<ph outputclass=\"packSize\">500</ph>\n<ph outputclass=\"unitOfMeasure\">gram</ph>\n<ph outputclass=\"legalCategory\">POM</ph>\r\n</p>\r\n<p outputclass=\"nhsIndicativeInfo\">\n<ph outputclass=\"nhsIndicative\">NHS indicative</ph>\n<ph outputclass=\"priceText\">price =</ph>\n<ph outputclass=\"nhsIndicativePrice\">&#163;49.95</ph>\n</p>\n</li>\r\n</ul>\n</section>\r\n<section outputclass=\"medicinalProduct\">\r\n<title>Calcium Resonium powder <ph outputclass=\"manufacturer\">(Sanofi)</ph>\n</title>\n<data name=\"ampid\">3812911000001109</dat