{
    "xml": "<topic id=\"PHP105337\" outputclass=\"medicinalForm\" type=\"medicinalForm\" namespace=\"/drugs/emollient-bath-and-shower-products-tar-containing/bath-additive\" basename=\"bath-additive\" title=\"Bath additive\">\n<title>Bath additive</title>\n<body>\r\n\r\n<section outputclass=\"excipients\">\r\n<title>Excipients</title>\r\n<p outputclass=\"introductoryText\">May contain</p>\r\n<p>Isopropyl palmitate, polysorbates</p>\n</section>\r\n\r\n<section outputclass=\"clinicalMedicinalProductInformationGroup\">\n<xref format=\"dita\" href=\"#PHP105733\" outputclass=\"clinicalMedicinalProductInformationXref\" namespace=\"/clinical-medicinal-product-information/polytar-emollient\">Polytar Emollient<tm tmtype=\"reg\"/>\r\n</xref>\r\n<xref format=\"dita\" href=\"#PHP105734\" outputclass=\"clinicalMedicinalProductInformationXref\" namespace=\"/clinical-medicinal-product-information/psoriderm-emulsion\">Psoriderm<tm tmtype=\"reg\"/> emulsion</xref>\n</section>\r\n<section outputclass=\"medicinalProduct\">\r\n<title>Psoriderm Emulsion 40% bath additive <ph outputclass=\"manufacturer\">(Dermal Laboratories Ltd)</ph>\n</title>\n<data name=\"ampid\">3820111000001101</data>\r\n<p outputclass=\"strengthOfActiveIngredient\">Coal tar distilled 400&#8239;mg per 1&#8239;ml</p>\r\n<ul outputclass=\"packs\">\r\n<li outputclass=\"packSizePriceTariff\">\r\n<p outputclass=\"packInfo\">\r\n<ph outputclass=\"packSize\">200</ph>\n<ph outputclass=\"unitOfMeasure\">ml</ph>\n<ph outputclass=\"legalCategory\">P</ph>\r\n</p>\r\n<p outputclass=\"nhsIndicativeInfo\">\n<ph outputclass=\"nhsIndicative\">NHS indicative</ph>\n<ph outputclass=\"priceText\">price =</ph>\n<ph outputclass=\"nhsIndicativePrice\">&#163;2.74</ph>\n</p>\n</li>\r\n</ul>\n</section>\r\n<section outputclass=\"medicinalProduct\">\r\n<title>Polytar Emollient <ph outputclass=\"manufacturer\">(GlaxoSmithKline Consumer Healthcare)</ph>\n</title>\n<data name=\"ampid\">3646411000001101</data>\r\n<p outputclass=\"strengthOfActiveIngredient\">Arachis oil extract of crude coal tar 75&#8239;mg per 1&#8239;gram</p>\r\n<p outputclass=\"strengthOfActiveIngredient\">Cade oil 75&#8239;mg per 1&#8239;gram</p>\r\n<p outputclass=\"strengthOfActiveIngredient\">Coal tar solution 25&#8239;mg per 1&#8239;gram</p>\r\n<p outputclass=\"strengthOfActiveIngredient\">Liquid paraffin light 350&#8239;mg per 1&#8239;gram</p>\r\n<p outputclass=\"strengthOfActiveIngredient\">Pine tar 75&#8239;mg per 1&#8239;gram</p>\r\n<ul outputclass=\"packs\">\r\n<li outputclass=\"packSizePriceTariff\">\r\n<p outputclass=\"packInfo\">\r\n<ph outputclass=\"packSize\">500</ph>\n<ph outputclass=\"unitOfMeasure\">ml</ph>\n<ph outputclass=\"legalCategory\">GSL</ph>\r\n</p>\r\n<p outputclass=\"nhsIndicativeInfo\">\n<ph outputclass=\"nhsIndicative\">NHS indicative</ph>\n<ph outputclass=\"priceText\">price =</ph>\n<ph outputclass=\"nhsIndicativePrice\">&#163;5.78</ph>\n</p>\n</li>\r\n</ul>\n</section>\r\n</body>\n<parents>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP105074\" title=\"EMOLLIENT BATH AND SHOWER PRODUCTS, TAR-CONTAINING\" namespace=\"/drugs/emollient-bath-and-shower-products-tar-containing\">EMOLLIENT BATH AND SHOWER PRODUCTS, TAR-CONTAINING</xref>\n</parents>\n</topic>",
