    "xml": "<topic id=\"PHP92378\" outputclass=\"medicinalForm\" type=\"medicinalForm\" namespace=\"/drugs/clindamycin-with-tretinoin/gel\" basename=\"gel\" title=\"Gel\">\n<title>Gel</title>\n<body>\n<section outputclass=\"cautionaryAndAdvisoryLabels\">\n<title>Cautionary and advisory labels</title>\n<p>Label <ph outputclass=\"labelNumber\">11</ph> - Protect your skin from sunlight&#8212;even on a bright but cloudy day. Do not use sunbeds </p>\n</section>\n<section outputclass=\"excipients\">\n<title>Excipients</title>\n<p outputclass=\"introductoryText\">May contain</p>\n<p>Butylated hydroxytoluene, hydroxybenzoates (parabens), polysorbates</p>\n</section>\n<section outputclass=\"medicinalProduct\">\n<title>Treclin 1%/0.025% gel <ph outputclass=\"manufacturer\">(Meda Pharmaceuticals Ltd)</ph>\n</title>\n<data name=\"ampid\">24432711000001108</data>\n<p outputclass=\"strengthOfActiveIngredient\">Clindamycin (as Clindamycin phosphate) 10&#8239;mg per 1&#8239;gram</p>\n<p outputclass=\"strengthOfActiveIngredient\">Tretinoin 250&#8239;microgram per 1&#8239;gram</p>\n<ul outputclass=\"packs\">\n<li outputclass=\"packSizePriceTariff\">\n<p outputclass=\"packInfo\">\n<ph outputclass=\"packSize\">30</ph>\n<ph outputclass=\"unitOfMeasure\">gram</ph>\n<ph outputclass=\"legalCategory\">POM</ph>\n</p>\n<p outputclass=\"nhsIndicativeInfo\">\n<ph outputclass=\"nhsIndicative\">NHS indicative</ph>\n<ph outputclass=\"priceText\">price =</ph>\n<ph outputclass=\"nhsIndicativePrice\">&#163;11.94</ph>\n</p>\n</li>\n</ul>\n</section>\n</body>\n<parents>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP88055\" title=\"CLINDAMYCIN WITH TRETINOIN\" namespace=\"/drugs/clindamycin-with-tretinoin\">CLINDAMYCIN WITH TRETINOIN</xref>\n</parents>\n</topic>",
