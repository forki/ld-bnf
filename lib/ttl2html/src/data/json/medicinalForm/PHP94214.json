    "xml": "<topic id=\"PHP94214\" outputclass=\"medicinalForm\" type=\"medicinalForm\" namespace=\"/drugs/magnesium-citrate-with-sodium-picosulfate/powder\" basename=\"powder\" title=\"Powder\">\n<title>Powder</title>\n<body>\r\n<section outputclass=\"cautionaryAndAdvisoryLabels\">\r\n<title>Cautionary and advisory labels</title>\r\n<p>Label <ph outputclass=\"labelNumber\">10</ph> - Warning: Read the additional information given with this medicine </p>\r\n<p>Label <ph outputclass=\"labelNumber\">13</ph> - Dissolve or mix with water before taking </p>\n</section>\r\n\r\n<section outputclass=\"electrolytes\">\r\n<title>Electrolytes</title>\r\n<p outputclass=\"introductoryText\">May contain</p>\r\n<p>Magnesium, potassium</p>\n</section>\r\n<section outputclass=\"clinicalMedicinalProductInformationGroup\">\n<xref format=\"dita\" href=\"#PHP104417\" outputclass=\"clinicalMedicinalProductInformationXref\" namespace=\"/clinical-medicinal-product-information/citrafleet-sachets\">CitraFleet<tm tmtype=\"reg\"/> sachets</xref>\r\n<xref format=\"dita\" href=\"#PHP104418\" outputclass=\"clinicalMedicinalProductInformationXref\" namespace=\"/clinical-medicinal-product-information/picolax-sachets\">Picolax<tm tmtype=\"reg\"/> sachets</xref>\n</section>\r\n<section outputclass=\"medicinalProduct\">\r\n<title>CitraFleet oral powder 15.08g sachets <ph outputclass=\"manufacturer\">(Laboratorios Casen-Fleet S.L.U)</ph>\n</title>\n<data name=\"ampid\">17068511000001106</data>\r\n<p outputclass=\"strengthOfActiveIngredient\">Citric acid anhydrous 10.97&#8239;gram</p>\r\n<p outputclass=\"strengthOfActiveIngredient\">Magnesium oxide light 3.5&#8239;gram</p>\r\n<p outputclass=\"strengthOfActiveIngredient\">Sodium picosulfate 10&#8239;mg</p>\r\n<ul outputclass=\"packs\">\r\n<li outputclass=\"packSizePriceTariff\">\r\n<p outputclass=\"packInfo\">\r\n<ph outputclass=\"packSize\">2</ph>\n<ph outputclass=\"unitOfMeasure\">sachet</ph>\n<ph outputclass=\"legalCategory\">P</ph>\r\n</p>\r\n<p outputclass=\"nhsIndicativeInfo\">\n<ph outputclass=\"nhsIndicative\">NHS indicative</ph>\n<ph outputclass=\"priceText\">price =</ph>\n<ph outputclass=\"nhsIndicativePrice\">&#163;3.25</ph>\n</p>\n</li>\r\n</ul>\n</section>\r\n<section outputclass=\"medicinalProduct\">\r\n<title>Picolax oral powder 16.1g sachets <ph outputclass=\"manufacturer\">(Ferring Pharmaceuticals Ltd)</ph>\n</title>\n<data name=\"ampid\">3916111000001103</data>\r\n<p outputclass=\"strengthOfActiveIngredient\">Citric acid anhydrous 12&#8239;gram</p>\r\n<p outputclass=\"strengthOfActiveIngredient\">Magnesium oxide 3.5&#8239;gram</p>\r\n<p outputclass=\"strengthOfActiveIngredient\">Sodium picosulfate 10&#8239;mg</p>\r\n<ul outputclass=\"packs\">\r\n<li outputclass=\"packSizePriceTariff\">\r\n<p outputclass=\"packInfo\">\r\n<ph outputclass=\"packSize\">2</ph>\n<ph outputclass=\"unitOfMeasure\">sachet</ph>\n<ph outputclass=\"legalCategory\">P</ph>\r\n</p>\r\n<p outputclass=\"nhsIndicativeInfo\">\n<ph outputclass=\"nhsIndicative\">NHS indicative</ph>\n<ph outputclass=\"priceText\">price =</ph>\n<ph outputclass=\"nhsIndicativePrice\">&#163;3.39</ph>\n</p>\n</li>\r\n</ul>\n</section>\r\n</body>\n<parents>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP661\" title=\"MAGNESIUM CITRATE WITH SODIUM PICOSULFATE\" namespace=\"/drugs/magnesium-citrate-with-sodium-picosulfate\">MAGNESIUM CITRATE WITH SODIUM PICOSULFATE</xref>\n</parents>\n</topic>",
