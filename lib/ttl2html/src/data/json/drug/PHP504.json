        "advice": [
            {
                "type": "advice",
                "title": "Gastro-intestinal side effects",
                "textContent": "Upper gastro-intestinal side-effects common over 4 g daily.",
                "html": "<p>Upper gastro-intestinal side-effects common over 4&#8239;g daily.</p>"
            },
            {
                "type": "advice",
                "title": "Blood disorders",
                "textContent": "Haematological abnormalities occur usually in the first 3 to 6 months of treatment and are reversible on cessation of treatment.",
                "html": "<p>Haematological abnormalities occur usually in the first 3 to 6 months of treatment and are reversible on cessation of treatment.</p>"
            }
        ]
    },
    "pregnancy": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Theoretical risk of neonatal haemolysis in third trimester; adequate folate supplements should be given to mother.",
                "html": "<p>Theoretical risk of neonatal haemolysis in third trimester; adequate folate supplements should be given to mother.</p>"
            }
        ]
    },
    "breastFeeding": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Small amounts in milk (1 report of bloody diarrhoea); theoretical risk of neonatal haemolysis especially in G6PD-deficient infants.",
                "html": "<p>Small amounts in milk (1 report of bloody diarrhoea); theoretical risk of neonatal haemolysis especially in G6PD-deficient infants.</p>"
            }
        ]
    },
    "hepaticImpairment": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Use with caution.",
                "html": "<p>Use with caution.</p>"
            }
        ]
    },
    "renalImpairment": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Risk of toxicity, including crystalluria, in moderate impairment&#8212;ensure high fluid intake.\n\nAvoid in severe impairment.",
                "html": "<p>Risk of toxicity, including crystalluria, in moderate impairment&#8212;ensure high fluid intake.</p><p>Avoid in severe impairment.</p>"
            }
        ]
    },
    "monitoringRequirements": {
        "patientParameters": [
            {
                "type": "patientParameters",
                "title": "Blood disorders",
                "textContent": "Close monitoring of full blood counts (including differential white cell count and platelet count) is necessary initially, and at monthly intervals during the first 3 months.",
                "html": "<p>Close monitoring of full blood counts (including differential white cell count and platelet count) is necessary initially, and at monthly intervals during the first 3 months.</p>"
            },
            {
                "type": "patientParameters",
                "title": "Renal function",
                "textContent": "Although the manufacturer recommends renal function tests in rheumatic diseases, evidence of practical value is unsatisfactory.",
                "html": "<p>Although the manufacturer recommends renal function tests in rheumatic diseases, evidence of practical value is unsatisfactory.</p>"
            },
            {
                "type": "patientParameters",
                "title": "Liver function",
                "textContent": "Liver function tests should be performed at monthly intervals for first 3 months.",
                "html": "<p> Liver function tests should be performed at monthly intervals for first 3 months.</p>"
            }
        ]
    },
