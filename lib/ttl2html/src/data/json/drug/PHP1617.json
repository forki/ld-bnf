    "xml": "<topic id=\"PHP1617\" outputclass=\"drug\" rev=\"1.11\" type=\"drug\" namespace=\"/drugs/protein-c-concentrate\" basename=\"protein-c-concentrate\" title=\"PROTEIN C CONCENTRATE\">\n<title>PROTEIN C CONCENTRATE</title>\n<body>\n<data name=\"vtmid\">391874000</data>\n<data name=\"classifications\">\n<data name=\"classification\">\n<data name=\"drugClassification\">\n<xref format=\"dita\" scope=\"local\" type=\"drugClassification\" href=\"#_270158130\" title=\"Blood Products\">Blood Products</xref>\n</data>\n</data>\n</data>\n<data name=\"primaryDomainOfEffect\">\n<data name=\"domainOfEffect\">  </data>\n</data>\n</body>\n<topic id=\"PHP57866\" outputclass=\"indicationsAndDose\" rev=\"1.16\" parent=\"/drugs/protein-c-concentrate\">\n<title>Indications and dose</title>\n<body>\n<section outputclass=\"indicationAndDoseGroup\">\n<sectiondiv outputclass=\"therapeuticIndications\">\n<p outputclass=\"therapeuticIndication\">Congenital protein C deficiency</p>\n</sectiondiv>\n<p outputclass=\"routesOfAdministration\">By intravenous injection</p>\n<ul outputclass=\"patientGroups\">\n<li outputclass=\"patientGroup adult\">\n<p>Adult</p>\n<p>(consult haematologist).</p>\n</li>\n</ul>\n</section>\n</body>\n</topic>\n<topic id=\"PHP57954\" outputclass=\"cautions\" rev=\"1.9\" parent=\"/drugs/protein-c-concentrate\">\n<title>Cautions</title>\n<body>\n<section outputclass=\"cautions\">\n<p>\n<ph outputclass=\"caution\">Hypersensitivity to heparins</ph>\n</p>\n</section>\n</body>\n</topic>\n<topic id=\"PHP57806\" outputclass=\"sideEffects\" rev=\"1.8\" parent=\"/drugs/protein-c-concentrate\">\n<title>Side-effects</title>\n<body>\n<section outputclass=\"generalSideEffects\">\n<sectiondiv outputclass=\"frequencies\">\n<sectiondiv outputclass=\"veryRare\">\n<p outputclass=\"title\">Very rare</p>\n<p outputclass=\"sideEffects\">\n<ph outputclass=\"sideEffect\">Bleeding</ph>; <ph outputclass=\"sideEffect\">dizziness</ph>; <ph outputclass=\"sideEffect\">fever</ph>; <ph outputclass=\"sideEffect\">hypersensitivity reactions</ph>\n</p>\n</sectiondiv>\n</sectiondiv>\n</section>\n</body>\n</topic>\n<topic id=\"PHP57303\" outputclass=\"prescribingAndDispensingInformation\" rev=\"1.8\" parent=\"/drugs/protein-c-concentrate\">\n<title>Prescribing and dispensing information</title>\n<body>\n<section>\n<sectiondiv>\n<p>Protein C is prepared from human plasma.</p>\n</sectiondiv>\n</section>\n</body>\n</topic>\n<topic id=\"PHP1617-medicinalForms\" outputclass=\"medicinalForms\" parent=\"/drugs/protein-c-concentrate\">\n<title>Medicinal forms</title>\n<body>\n<section outputclass=\"licensingVariationStatement\">\n<p>There can be variation in the licensing of different medicines containing the same drug.</p>\n</section>\n</body>\n</topic>\n<parents>\n<xref format=\"dita\" scope=\"local\" type=\"drugs\" href=\"#drugs\" title=\"Drugs\" namespace=\"/drugs\">Drugs</xref>\n</parents>\n<children>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP76818\" title=\"Powder and solvent for solution for injection\" namespace=\"/drugs/protein-c-concentrate/powder-and-solvent-for-solution-for-injection\">Powder and solvent for solution for injection</xref>\n</children>\n<backlinks>\n<xref format=\"dita\" scope=\"local\" type=\"drugClassifications\" href=\"#PHP106980\" namespace=\"/drug-classifications\" title=\"Drug classifications\" count=\"1\" rel=\"backlink\">Drug classifications</xref>\n</backlinks>\n<links>\n<xref format=\"dita\" scope=\"local\" type=\"#drugs\" href=\"#drugs\" namespace=\"/drugs\" title=\"Drugs\" count=\"1\" rel=\"link\">Drugs</xref>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP76818\" namespace=\"/drugs/protein-c-concentrate/powder-and-solvent-for-solution-for-injection\" title=\"Powder and solvent for solution for injection\" count=\"1\" rel=\"link\">Powder and solvent for solution for injection</xref>\n</links>\n</topic>",
