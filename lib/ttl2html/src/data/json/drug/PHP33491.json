    "monitoringRequirements": {
        "patientParameters": [
            {
                "type": "patientParameters",
                "specificity": {
                    "textContent": "With intravitreal use in adults",
                    "html": "With <ph outputclass=\"route\">intravitreal</ph> use in adults",
                    "patientType": "adults",
                    "routes": [
                        "intravitreal"
                    ]
                },
                "textContent": "Monitor for raised intra-ocular pressure (particularly if raised at baseline), retinal detachment, endophthalmitis, vitreous haemorrhage or detachment within 2&#8211;7 days following the procedure.\n\nMonitor intra-ocular pressure at least every 3 months thereafter (for approximately 36 months).",
                "html": "<p>Monitor for raised intra-ocular pressure (particularly if raised at baseline), retinal detachment, endophthalmitis, vitreous haemorrhage or detachment within 2&#8211;7 days following the procedure.</p><p>Monitor intra-ocular pressure at least every 3 months thereafter (for approximately 36 months).</p>"
            }
        ]
    },
