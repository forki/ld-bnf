    },
    "breastFeeding": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Manufacturer advises avoid&#8212;present in milk in animal studies.",
                "html": "<p>Manufacturer advises avoid&#8212;present in milk in <i>animal</i> studies.</p>"
            }
        ]
    },
    "hepaticImpairment": {
        "doseAdjustments": [
            {
                "type": "doseAdjustments",
                "textContent": "Use with caution in moderate impairment, reduce dose considerably then gradually increase to max. 50% of normal dose.",
                "html": "<p>Use with caution in moderate impairment, reduce dose considerably then gradually increase to max. 50% of normal dose.</p>"
            }
        ],
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Avoid in severe impairment.",
                "html": "<p>Avoid in severe impairment.</p>"
            }
        ]
    },
    "renalImpairment": {
        "doseAdjustments": [
            {
                "type": "doseAdjustments",
                "textContent": "Reduce dose by 10 mg/kg if eGFR 60&#8211;90 mL/minute/1.73 m2 and if serum creatinine increased by more than 33% of baseline measurement on 2 consecutive occasions&#8212;interrupt treatment if deterioration in renal function persists after dose reduction.",
                "html": "<p>Reduce dose by 10&#8239;mg/kg if eGFR 60&#8211;90&#8239;mL/minute/1.73&#8239;m<sup>2</sup> and if serum creatinine increased by more than 33% of baseline measurement on 2 consecutive occasions&#8212;interrupt treatment if deterioration in renal function persists after dose reduction.</p>"
            }
        ],
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Avoid if eGFR less than 60 mL/minute/1.73 m2.",
                "html": "<p>Avoid if eGFR less than 60&#8239;mL/minute/1.73&#8239;m<sup>2</sup>.</p>"
            }
        ]
    },
    "monitoringRequirements": {
        "patientParameters": [
            {
                "type": "patientParameters",
                "textContent": "Eye and ear examinations required before treatment and annually during treatment.\n\nMonitor body-weight, height, and sexual development in children annually.\n\nMonitor serum-ferritin concentration monthly.\n\nTest liver function before treatment, then every 2 weeks during the first month, and then monthly.\n\nMeasure baseline serum creatinine and monitor renal function weekly during the first month of treatment and monthly thereafter.\n\nTest for proteinuria monthly.",
                "html": "<p>Eye and ear examinations required before treatment and annually during treatment.</p><p>Monitor body-weight, height, and sexual development in children annually.</p><p>Monitor serum-ferritin concentration monthly.</p><p>Test liver function before treatment, then every 2 weeks during the first month, and then monthly.</p><p>Measure baseline serum creatinine and monitor renal function weekly during the first month of treatment and monthly thereafter.</p><p>Test for proteinuria monthly.</p>"
            }
        ]
    },
    "directionsForAdministration": {
        "directionsForAdministration": [
            {
                "type": "directionsForAdministration",
                "textContent": "Tablets should be dispersed in water, orange juice, or apple juice; if necessary resuspend residue.",
                "html": "<p>Tablets should be dispersed in water, orange juice, or apple juice; if necessary resuspend residue.</p>"
            }
        ]
    },
    "patientAndCarerAdvice": {
        "generalPatientAdvice": [
            {
                "type": "generalPatientAdvice",
                "textContent": "Patient or carers should be given advice on how to administer deferasirox dispersible tablets.",
                "html": "<p>Patient or carers should be given advice on how to administer deferasirox dispersible tablets.</p>"
            }
        ]
    },
    "nationalFunding": {
        "smcDecisions": [
            {
                "type": "smcDecisions",
                "textContent": "The Scottish Medicines Consortium has advised (January 2007) that deferasirox is accepted for restricted use within NHS Scotland for the treatment of chronic iron overload associated with the treatment of rare acquired or inherited anaemias requiring recurrent blood transfusions. It is not recommended for patients with myelodysplastic syndromes.",
                "html": "<p>The <i>Scottish Medicines Consortium</i> has advised (January 2007) that deferasirox is accepted for restricted use within NHS Scotland for the treatment of chronic iron overload associated with the treatment of rare acquired or inherited anaemias requiring recurrent blood transfusions. It is not recommended for patients with myelodysplastic syndromes.</p>"
            }
        ]
    },
