    "xml": "<topic id=\"PHP33749\" outputclass=\"drug\" rev=\"1.12\" type=\"drug\" namespace=\"/drugs/soybean-oil\" basename=\"soybean-oil\" title=\"SOYBEAN OIL\">\n<title>SOYBEAN OIL</title>\n<body>\n<data name=\"vtmid\">420116007</data>\n<data name=\"classifications\">\n<data name=\"classification\">\n<data name=\"drugClassification\">\n<xref format=\"dita\" scope=\"local\" type=\"drugClassification\" href=\"#_354209452\" title=\"Ocular lubricants\">Ocular lubricants</xref>\n</data>\n</data>\n</data>\n<data name=\"primaryDomainOfEffect\">\n<data name=\"domainOfEffect\">  </data>\n</data>\n</body>\n<topic id=\"PHP68191\" outputclass=\"indicationsAndDose\" rev=\"1.16\" parent=\"/drugs/soybean-oil\">\n<title>Indications and dose</title>\n<body>\n<section outputclass=\"indicationAndDoseGroup\">\n<sectiondiv outputclass=\"therapeuticIndications\">\n<p outputclass=\"therapeuticIndication\">Dry eye conditions</p>\n</sectiondiv>\n<p outputclass=\"routesOfAdministration\">To the eye</p>\n<ul outputclass=\"patientGroups\">\n<li outputclass=\"patientGroup child\">\n<p>Child</p>\n<p>Apply up to 4 times a day.</p>\n</li>\n<li outputclass=\"patientGroup adult\">\n<p>Adult</p>\n<p>Apply up to 4 times a day.</p>\n</li>\n</ul>\n</section>\n</body>\n</topic>\n<topic id=\"PHP33749-medicinalForms\" outputclass=\"medicinalForms\" parent=\"/drugs/soybean-oil\">\n<title>Medicinal forms</title>\n<body>\n<section outputclass=\"licensingVariationStatement\">\n<p>There can be variation in the licensing of different medicines containing the same drug.</p>\n</section>\n</body>\n</topic>\n<parents>\n<xref format=\"dita\" scope=\"local\" type=\"drugs\" href=\"#drugs\" title=\"Drugs\" namespace=\"/drugs\">Drugs</xref>\n</parents>\n<children>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP94207\" title=\"Emulsion for injection\" namespace=\"/drugs/soybean-oil/emulsion-for-injection\">Emulsion for injection</xref>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP94208\" title=\"Bath additive\" namespace=\"/drugs/soybean-oil/bath-additive\">Bath additive</xref>\n</children>\n<backlinks>\n<xref format=\"dita\" scope=\"local\" type=\"drugClassifications\" href=\"#PHP106980\" namespace=\"/drug-classifications\" title=\"Drug classifications\" count=\"1\" rel=\"backlink\">Drug classifications</xref>\n</backlinks>\n<links>\n<xref format=\"dita\" scope=\"local\" type=\"#drugs\" href=\"#drugs\" namespace=\"/drugs\" title=\"Drugs\" count=\"1\" rel=\"link\">Drugs</xref>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP94207\" namespace=\"/drugs/soybean-oil/emulsion-for-injection\" title=\"Emulsion for injection\" count=\"1\" rel=\"link\">Emulsion for injection</xref>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP94208\" namespace=\"/drugs/soybean-oil/bath-additive\" title=\"Bath additive\" count=\"1\" rel=\"link\">Bath additive</xref>\n</links>\n</topic>",
