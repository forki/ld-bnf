    "xml": "<topic id=\"PHP106449\" outputclass=\"drug\" rev=\"1.18\" type=\"drug\" namespace=\"/drugs/barrier-creams-and-ointments\" basename=\"barrier-creams-and-ointments\" title=\"BARRIER CREAMS AND OINTMENTS\">\n<title>BARRIER CREAMS AND OINTMENTS</title>\n<body>\n<data name=\"classifications\">\n<data name=\"classification\">\n<data name=\"drugClassification\">\n<xref format=\"dita\" scope=\"local\" type=\"drugClassification\" href=\"#_350344367\" title=\"Barrier preparations\">Barrier preparations</xref>\n</data>\n</data>\n</data>\n</body>\n<topic id=\"PHP106595\" outputclass=\"indicationsAndDose\" rev=\"1.11\" parent=\"/drugs/barrier-creams-and-ointments\">\n<title>Indications and dose</title>\n<body>\n<section outputclass=\"indicationAndDoseGroup\">\n<sectiondiv outputclass=\"therapeuticIndications\">\n<p outputclass=\"therapeuticIndication\">For use as a barrier preparation</p>\n</sectiondiv>\n<p outputclass=\"routesOfAdministration\">To the skin</p>\n<ul outputclass=\"patientGroups\">\n<li outputclass=\"patientGroup child\">\n<p>Child</p>\n<p>(consult product literature).</p>\n</li>\n<li outputclass=\"patientGroup adult\">\n<p>Adult</p>\n<p>(consult product literature).</p>\n</li>\n</ul>\n</section>\n</body>\n</topic>\n<topic id=\"PHP106449-medicinalForms\" outputclass=\"medicinalForms\" parent=\"/drugs/barrier-creams-and-ointments\">\n<title>Medicinal forms</title>\n<body>\n<section outputclass=\"licensingVariationStatement\">\n<p>There can be variation in the licensing of different medicines containing the same drug.</p>\n</section>\n</body>\n</topic>\n<parents>\n<xref format=\"dita\" scope=\"local\" type=\"drugs\" href=\"#drugs\" title=\"Drugs\" namespace=\"/drugs\">Drugs</xref>\n</parents>\n<children>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP106624\" title=\"Cream\" namespace=\"/drugs/barrier-creams-and-ointments/cream\">Cream</xref>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP106623\" title=\"Ointment\" namespace=\"/drugs/barrier-creams-and-ointments/ointment\">Ointment</xref>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP106625\" title=\"Spray\" namespace=\"/drugs/barrier-creams-and-ointments/spray\">Spray</xref>\n</children>\n<backlinks>\n<xref format=\"dita\" scope=\"local\" type=\"drugClassifications\" href=\"#PHP106980\" namespace=\"/drug-classifications\" title=\"Drug classifications\" count=\"1\" rel=\"backlink\">Drug classifications</xref>\n</backlinks>\n<links>\n<xref format=\"dita\" scope=\"local\" type=\"#drugs\" href=\"#drugs\" namespace=\"/drugs\" title=\"Drugs\" count=\"1\" rel=\"link\">Drugs</xref>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP106624\" namespace=\"/drugs/barrier-creams-and-ointments/cream\" title=\"Cream\" count=\"1\" rel=\"link\">Cream</xref>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP106623\" namespace=\"/drugs/barrier-creams-and-ointments/ointment\" title=\"Ointment\" count=\"1\" rel=\"link\">Ointment</xref>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP106625\" namespace=\"/drugs/barrier-creams-and-ointments/spray\" title=\"Spray\" count=\"1\" rel=\"link\">Spray</xref>\n</links>\n</topic>",
