    "monitoringRequirements": {
        "patientParameters": [
            {
                "type": "patientParameters",
                "textContent": "Obtain ECG as soon as possible after starting treatment then continue monitoring in those taking medicines that increase plasma-piperaquine concentration, in children who are vomiting, in females, or in the elderly.\n\nConsider obtaining ECG in all patients before third dose and 4&#8211;6 hours after third dose.\n\nIf QTC interval more than 500 milliseconds, discontinue treatment and monitor ECG for a further 24&#8211;48 hours.",
                "html": "<p>Obtain ECG as soon as possible after starting treatment then continue monitoring in those taking medicines that increase plasma-piperaquine concentration, in children who are vomiting, in females, or in the elderly.</p><p>Consider obtaining ECG in all patients before third dose and 4&#8211;6 hours after third dose.</p><p>If QT<sub>C</sub> interval more than 500 milliseconds, discontinue treatment and monitor ECG for a further 24&#8211;48 hours.</p>"
            }
        ]
    },
    "directionsForAdministration": {
        "directionsForAdministration": [
            {
                "type": "directionsForAdministration",
                "textContent": "Tablets to be taken at least 3 hours before and at least 3 hours after food.\n\nTablets may be crushed and mixed with water immediately before administration.",
                "html": "<p>Tablets to be taken at least 3 hours before and at least 3 hours after food.</p><p>Tablets may be crushed and mixed with water immediately before administration.</p>"
            }
        ]
    },
