    "renalImpairment": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "specificity": {
                    "textContent": "In adults",
                    "html": "In adults",
                    "patientType": "adults"
                },
                "textContent": "Avoid (except for specialised role in some forms of renal disease).",
