        "overdosageInformation": [
            {
                "type": "overdosageInformation",
                "references": [
                    {
                        "id": "PHP78558",
                        "label": "Emergency treatment of poisoning"
                    }
                ],
                "textContent": "Quinine is very toxic in overdosage; life-threatening features include arrhythmias (which can have a very rapid onset) and convulsions (which can be intractable).\n\nFor details on the management of poisoning, see Emergency treatment of poisoning.",
                "html": "<p>Quinine is very toxic in overdosage; life-threatening features include arrhythmias (which can have a very rapid onset) and convulsions (which can be intractable).</p><p>For details on the management of poisoning, see <xref format=\"dita\" href=\"#PHP78558\" type=\"bookmark\" namespace=\"/treatment-summaries/emergency-treatment-of-poisoning\">Emergency treatment of poisoning</xref>.</p>"
            }
        ]
    },
    "pregnancy": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "High doses are teratogenic in first trimester, but in malaria benefit of treatment outweighs risk.",
                "html": "<p>High doses are teratogenic in <i>first trimester</i>, but in malaria benefit of treatment outweighs risk.</p>"
            }
        ]
    },
    "breastFeeding": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Present in milk but not known to be harmful.",
                "html": "<p>Present in milk but not known to be harmful.</p>"
            }
        ]
    },
    "hepaticImpairment": {
        "doseAdjustments": [
            {
                "type": "doseAdjustments",
                "specificity": {
                    "textContent": "With intravenous use",
                    "html": "With <ph outputclass=\"route\">intravenous</ph> use",
                    "routes": [
                        "intravenous"
                    ]
                },
                "textContent": "For treatment of malaria in severe impairment, reduce parenteral maintenance dose to 5&#8211;7 mg/kg of quinine salt.",
                "html": "<p>For treatment of malaria in severe impairment, reduce parenteral maintenance dose to 5&#8211;7&#8239;mg/kg of quinine salt.</p>"
            }
        ]
    },
    "renalImpairment": {
        "doseAdjustments": [
            {
                "type": "doseAdjustments",
                "specificity": {
                    "textContent": "With intravenous use",
                    "html": "With <ph outputclass=\"route\">intravenous</ph> use",
                    "routes": [
                        "intravenous"
                    ]
                },
                "textContent": "For treatment of malaria in severe impairment, reduce parenteral maintenance dose to 5&#8211;7 mg/kg of quinine salt.",
                "html": "<p>For treatment of malaria in severe impairment, reduce parenteral maintenance dose to 5&#8211;7&#8239;mg/kg of quinine salt.</p>"
            }
        ]
    },
    "monitoringRequirements": {
        "patientParameters": [
            {
                "type": "patientParameters",
                "specificity": {
                    "textContent": "With intravenous use",
                    "html": "With <ph outputclass=\"route\">intravenous</ph> use",
                    "routes": [
                        "intravenous"
                    ]
                },
                "textContent": "Monitor blood glucose and electrolyte concentration during parenteral treatment.",
                "html": "<p>Monitor blood glucose and electrolyte concentration during parenteral treatment.</p>"
            },
            {
                "type": "patientParameters",
                "specificity": {
                    "textContent": "In adults",
                    "html": "In adults",
                    "patientType": "adults"
                },
                "textContent": "Patients taking quinine for nocturnal leg cramps should be monitored closely during the early stages for adverse effects as well as for benefit.",
                "html": "<p>Patients taking quinine for nocturnal leg cramps should be monitored closely during the early stages for adverse effects as well as for benefit.</p>"
            }
        ]
    },
    "directionsForAdministration": {
        "directionsForAdministration": [
            {
                "type": "directionsForAdministration",
                "specificity": {
                    "textContent": "With intravenous use in children",
                    "html": "With <ph outputclass=\"route\">intravenous</ph> use in children",
                    "patientType": "children",
                    "routes": [
                        "intravenous"
                    ]
                },
                "textContent": "For intravenous infusion, dilute to a concentration of 2 mg/mL (max. 30 mg/mL in fluid restriction) with Glucose 5% or Sodium Chloride 0.9% and give over 4 hours.",
                "html": "<p>For <i>intravenous infusion</i>, dilute to a concentration of 2&#8239;mg/mL (max. 30&#8239;mg/mL in fluid restriction) with Glucose 5% <i>or</i> Sodium Chloride 0.9% and give over 4 hours.</p>"
            },
            {
                "type": "directionsForAdministration",
                "specificity": {
                    "textContent": "With intravenous use in adults",
                    "html": "With <ph outputclass=\"route\">intravenous</ph> use in adults",
                    "patientType": "adults",
                    "routes": [
                        "intravenous"
                    ]
                },
                "textContent": "For intravenous infusion, give continuously in Glucose 5% or Sodium Chloride 0.9%. To be given over 4 hours.",
                "html": "<p>For <i>intravenous infusion</i>, give continuously in Glucose 5% or Sodium Chloride 0.9%. To be given over 4 hours.</p>"
            }
        ]
    },
    "prescribingAndDispensingInformation": {
        "prescribingAndDispensingInformation": [
            {
                "type": "prescribingAndDispensingInformation",
                "specificity": {
                    "textContent": "With intravenous use",
                    "html": "With <ph outputclass=\"route\">intravenous</ph> use",
                    "routes": [
                        "intravenous"
                    ]
                },
                "textContent": "Intravenous injection of quinine is so hazardous that it has been superseded by infusion.",
                "html": "<p>Intravenous injection of quinine is so hazardous that it has been superseded by infusion.</p>"
            }
        ]
    },
