    "hepaticImpairment": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Manufacturer advises caution in severe hepatic impairment&#8212;no information available.",
                "html": "<p>Manufacturer advises caution in severe hepatic impairment&#8212;no information available.</p>"
            }
