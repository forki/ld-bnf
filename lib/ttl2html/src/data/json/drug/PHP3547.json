    "breastFeeding": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Present in milk in small amounts, but absorption from gastrointestinal tract negligible.",
                "html": "<p>Present in milk in small amounts, but absorption from gastrointestinal tract negligible.</p>"
