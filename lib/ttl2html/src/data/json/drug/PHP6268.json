    "xml": "<topic id=\"PHP6268\" outputclass=\"drug\" rev=\"1.17\" type=\"drug\" namespace=\"/drugs/dihydrotachysterol\" basename=\"dihydrotachysterol\" title=\"DIHYDROTACHYSTEROL\">\n<title>DIHYDROTACHYSTEROL</title>\n<body>\r\n<p outputclass=\"interactionsLinks\"> List of individual interactants: <xref format=\"dita\" href=\"#bnf_int_1353\" namespace=\"/interactions/list-of-drug-interactions/vitamins/dihydrotachysterol\">Dihydrotachysterol</xref>\n</p>\n<data name=\"vtmid\">20237006</data>\n<data name=\"classifications\">\n<data name=\"classification\">\n<data name=\"drugClassification\">\n<xref format=\"dita\" scope=\"local\" type=\"drugClassification\" href=\"#_616977678\" title=\"Vitamin D and analogues\">Vitamin D and analogues</xref>\n</data>\n<data name=\"inheritsFromClass\">\n<xref format=\"dita\" scope=\"local\" type=\"drugClass\" href=\"#PHP34566\" title=\"VITAMIN D AND ANALOGUES (SYSTEMIC)\" namespace=\"/drug-classes/vitamin-d-and-analogues-systemic\">VITAMIN D AND ANALOGUES (SYSTEMIC)</xref>\n</data>\n</data>\n</data>\n<data name=\"primaryDomainOfEffect\">\n<data name=\"domainOfEffect\">  </data>\n</data>\r\n</body>\n<topic id=\"PHP35752\" outputclass=\"indicationsAndDose\" rev=\"1.14\" parent=\"/drugs/dihydrotachysterol\">\n<title>Indications and dose</title>\n<body>\n<section outputclass=\"indicationAndDoseGroup\">\n<sectiondiv outputclass=\"therapeuticIndications\">\n<p outputclass=\"therapeuticIndication\">Acute, chronic, and latent forms of hypocalcaemic tetany due to hypoparathyroidism</p>\n</sectiondiv>\n<p outputclass=\"routesOfAdministration\">By mouth</p>\n<ul outputclass=\"patientGroups\">\n<li outputclass=\"patientGroup adult\">\n<p>Adult</p>\n<p>(consult product literature).</p>\n</li>\n</ul>\n</section>\n</body>\n</topic>\n<topic id=\"PHP35731\" outputclass=\"monitoringRequirements\" parent=\"/drugs/dihydrotachysterol\">\n<title>Monitoring requirements</title>\n<body>\n<section outputclass=\"monitoringOfPatientParameters\">\n<sectiondiv>\