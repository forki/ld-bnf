    "xml": "<topic id=\"PHP88052\" outputclass=\"drug\" rev=\"1.18\" type=\"drug\" namespace=\"/drugs/erythromycin-with-zinc-acetate\" basename=\"erythromycin-with-zinc-acetate\" title=\"ERYTHROMYCIN WITH ZINC ACETATE\">\n<title>ERYTHROMYCIN WITH ZINC ACETATE</title>\n<body>\n<data name=\"vtmid\">398835002</data>\n<data name=\"classifications\">\n<data name=\"classification\">\n<data name=\"drugClassification\">\n<xref format=\"dita\" scope=\"local\" type=\"drugClassification\" href=\"#_616564198\" title=\"Macrolides and related drugs\">Macrolides and related drugs</xref>\n</data>\n</data>\n<data name=\"classification\">\n<data name=\"drugClassification\">\n<xref format=\"dita\" scope=\"local\" type=\"drugClassification\" href=\"#_947202787\" title=\"Astringents\">Astringents</xref>\n</data>\n</data>\n<data name=\"classification\">\n<data name=\"drugClassification\">\n<xref format=\"dita\" scope=\"local\" type=\"drugClassification\" href=\"#_645143129\" title=\"Copper absorption inhibitors\">Copper absorption inhibitors</xref>\n</data>\n</data>\n</data>\n<data name=\"primaryDomainOfEffect\">\n<data name=\"domainOfEffect\">  </data>\n</data>\n</body>\n<topic id=\"PHP89979\" outputclass=\"indicationsAndDose\" rev=\"1.19\" parent=\"/drugs/erythromycin-with-zinc-acetate\">\n<title>Indications and dose</title>\n<body>\n<section outputclass=\"indicationAndDoseGroup\">\n<sectiondiv outputclass=\"therapeuticIndications\">\n<p outputclass=\"therapeuticIndication\">Acne vulgaris</p>\n</sectiondiv>\n<p outputclass=\"routesOfAdministration\">To the skin</p>\n<ul outputclass=\"patientGroups\">\n<li outputclass=\"patientGroup child\">\n<p>Child</p>\n<p>Apply twice daily.</p>\n</li>\n<li outputclass=\"patientGroup adult\">\n<p>Adult</p>\n<p>Apply twice daily.</p>\n</li>\n</ul>\n</section>\n</body>\n</topic>\n<topic id=\"PHP89972\" outputclass=\"cautions\" rev=\"1.9\" parent=\"/drugs/erythromycin-with-zinc-acetate\">\n<title>Cautions</title>\n<body>\n<section outputclass=\"cautions\">\n<p>\n<ph outputclass=\"caution\">Some manufacturers advise preparations containing alcohol are not suitable for use with benzoyl peroxide</ph>\n</p>\n</section>\n</body>\n</topic>\n<topic id=\"PHP88052-medicinalForms\" outputclass=\"medicinalForms\" parent=\"/drugs/erythromycin-with-zinc-acetate\">\n<title>Medicinal forms</title>\n<body>\n<section outputclass=\"licensingVariationStatement\">\n<p>There can be variation in the licensing of different medicines containing the same drug.</p>\n</section>\n</body>\n</topic>\n<parents>\n<xref format=\"dita\" scope=\"local\" type=\"drugs\" href=\"#drugs\" title=\"Drugs\" namespace=\"/drugs\">Drugs</xref>\n</parents>\n<children>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP92375\" title=\"Liquid\" namespace=\"/drugs/erythromycin-with-zinc-acetate/liquid\">Liquid</xref>\n</children>\n<backlinks>\n<xref format=\"dita\" scope=\"local\" type=\"drugClassifications\" href=\"#PHP106980\" namespace=\"/drug-classifications\" title=\"Drug classifications\" count=\"3\" rel=\"backlink\">Drug classifications</xref>\n</backlinks>\n<links>\n<xref format=\"dita\" scope=\"local\" type=\"#drugs\" href=\"#drugs\" namespace=\"/drugs\" title=\"Drugs\" count=\"1\" rel=\"link\">Drugs</xref>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP92375\" namespace=\"/drugs/erythromycin-with-zinc-acetate/liquid\" title=\"Liquid\" count=\"1\" rel=\"link\">Liquid</xref>\n</links>\n</topic>",
