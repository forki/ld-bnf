    "xml": "<topic id=\"PHP100016\" outputclass=\"drug\" rev=\"1.18\" type=\"drug\" namespace=\"/drugs/coal-tar-with-coconut-oil-and-salicylic-acid\" basename=\"coal-tar-with-coconut-oil-and-salicylic-acid\" title=\"COAL TAR WITH COCONUT OIL AND SALICYLIC ACID\">\n<title>COAL TAR WITH COCONUT OIL AND SALICYLIC ACID</title>\n<body>\r\n<p outputclass=\"constituentDrugs\">The properties listed below are those particular to the combination only. For the properties of the components please consider, <xref format=\"dita\" href=\"#PHP7754\" namespace=\"/drugs/salicylic-acid\">salicylic acid</xref>, <xref format=\"dita\" href=\"#PHP105665\" namespace=\"/drugs/coal-tar\">coal tar</xref>.</p>\n<data name=\"classifications\">\n<data name=\"classification\">\n<data name=\"drugClassification\">\n<xref format=\"dita\" scope=\"local\" type=\"drugClassification\" href=\"#_219184260\" title=\"Tars\">Tars</xref>\n</data>\n</data>\r\n<data name=\"classification\">\n<data name=\"drugClassification\">\n<xref format=\"dita\" scope=\"local\" type=\"drugClassification\" href=\"#_470968003\" title=\"Fatty acids\">Fatty acids</xref>\n</data>\n</data>\r\n<data name=\"classification\">\n<data name=\"drugClassification\">\n<xref format=\"dita\" scope=\"local\" type=\"drugClassification\" href=\"#_904822154\" title=\"Salicylates\">Salicylates</xref>\n</data>\n</data>\n</data>\n<data name=\"primaryDomainOfEffect\">\n<data name=\"domainOfEffect\">  </data>\n</data>\r\n</body>\n<topic id=\"PHP100059\" outputclass=\"indicationsAndDose\" rev=\"1.27\" parent=\"/drugs/coal-tar-with-coconut-oil-and-salicylic-acid\">\n<title>Indications and dose</title>\n<body>\n<section outputclass=\"indicationAndDoseGroup\">\n<sectiondiv outputclass=\"therapeuticIndications\">\n<p outputclass=\"therapeuticIndication\">Scaly scalp disorders</p>\n<p outputclass=\"therapeuticIndication\">Psoriasis</p>\n<p outputclass=\"therapeuticIndication\">Seborrhoeic dermatitis</p>\n<p outputclass=\"therapeuticIndication\">Dandruff</p>\n<p outputclass=\"therapeuticIndication\">Cradle cap</p>\n</sectiondiv>\n<p outputclass=\"routesOfAdministration\">To the skin using shampoo</p>\n<ul outputclass=\"patientGroups\">\n<li outputclass=\"patientGroup child\">\n<p>Child</p>\n<p>Apply daily as required.</p>\n</li>\n<li outputclass=\"patientGroup adult\">\n<p>Adult</p>\n<p>Apply daily as required.</p>\n</li>\n</ul>\n</section>\n</body>\n</topic>\n<topic id=\"PHP100016-medicinalForms\" outputclass=\"medicinalForms\" parent=\"/drugs/coal-tar-with-coconut-oil-and-salicylic-acid\">\n<title>Medicinal forms</title>\n<body>\n<section outputclass=\"licensingVariationStatement\">\n<p>There can be variation in the licensing of different medicines containing the same drug.</p>\n</section>\n</body>\n</topic>\n<parents>\n<xref format=\"dita\" scope=\"local\" type=\"drugs\" href=\"#drugs\" title=\"Drugs\" namespace=\"/drugs\">Drugs</xref>\n</parents>\n<children>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP00001\" title=\"Shampoo\" namespace=\"/drugs/coal-tar-with-coconut-oil-and-salicylic-acid/shampoo\">Shampoo</xref>\n</children>\n<backlinks>\n<xref format=\"dita\" scope=\"local\" type=\"drugClassifications\" href=\"#PHP106980\" namespace=\"/drug-classifications\" title=\"Drug classifications\" count=\"3\" rel=\"backlink\">Drug classifications</xref>\n</backlinks>\n<links>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP7754\" namespace=\"/drugs/salicylic-acid\" title=\"SALICYLIC ACID\" count=\"1\" rel=\"link\">SALICYLIC ACID</xref>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP105665\" namespace=\"/drugs/coal-tar\" title=\"COAL TAR\" count=\"1\" rel=\"link\">COAL TAR</xref>\n<xref format=\"dita\" scope=\"local\" type=\"#drugs\" href=\"#drugs\" namespace=\"/drugs\" title=\"Drugs\" count=\"1\" rel=\"link\">Drugs</xref>\n<xref format=\"dita\" scope=\"local\" type=\"medicinalForm\" href=\"#PHP00001\" namespace=\"/drugs/coal-tar-with-coconut-oil-and-salicylic-acid/shampoo\" title=\"Shampoo\" count=\"1\" rel=\"link\">Shampoo</xref>\n</links>\n</topic>",
