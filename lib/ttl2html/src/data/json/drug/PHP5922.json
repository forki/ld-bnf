    "conceptionAndContraception": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Manufacturer advises effective contraception during and for 5 months after treatment.",
                "html": "<p>Manufacturer advises effective contraception during and for 5 months after treatment.</p>"
            }
        ]
    },
    "pregnancy": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "No information available&#8212;use only if potential benefit outweighs risk.\n\nHuman IgG antibodies known to cross placenta.",
                "html": "<p>No information available&#8212;use only if potential benefit outweighs risk.</p><p>Human IgG antibodies known to cross placenta.</p>"
            }
        ]
    },
    "breastFeeding": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "No information available&#8212;manufacturer advises avoid breast-feeding during and for 5 months after treatment.",
                "html": "<p>No information available&#8212;manufacturer advises avoid breast-feeding during and for 5 months after treatment.</p>"
            }
        ]
    },
    "monitoringRequirements": {
        "patientParameters": [
            {
                "type": "patientParameters",
                "textContent": "Monitor for 1 hour after infusion.",
                "html": "<p>Monitor for 1 hour after infusion.</p>"
            },
            {
                "type": "patientParameters",
                "textContent": "For paroxysmal nocturnal haemoglobinuria, monitor for intravascular haemolysis (including serum-lactate dehydrogenase concentration) during treatment and for at least 8 weeks after discontinuation.",
                "html": "<p>For <i>paroxysmal nocturnal haemoglobinuria</i>, monitor for intravascular haemolysis (including serum-lactate dehydrogenase concentration) during treatment and for at least 8 weeks after discontinuation.</p>"
            },
            {
                "type": "patientParameters",
                "textContent": "For atypical haemolytic uraemic syndrome, monitor for thrombotic microangiopathy (measure platelet count, serum-lactate dehydrogenase concentration, and serum creatinine) during treatment and for at least 12 weeks after discontinuation.",
                "html": "<p>For <i>atypical haemolytic uraemic syndrome</i>, monitor for thrombotic microangiopathy (measure platelet count, serum-lactate dehydrogenase concentration, and serum creatinine) during treatment and for at least 12 weeks after discontinuation.</p>"
            }
        ]
    },
    "directionsForAdministration": {
        "directionsForAdministration": [
            {
                "type": "directionsForAdministration",
                "specificity": {
                    "textContent": "With intravenous use",
                    "html": "With <ph outputclass=\"route\">intravenous</ph> use",
                    "routes": [
                        "intravenous"
                    ]
                },
                "textContent": "For intravenous infusion (Soliris ), give intermittently in Glucose 5% or Sodium chloride 0.9%. Dilute requisite dose to a concentration of 5 mg/mL and mix gently; give over 25&#8211;45 minutes (infusion time may be increased to 2 hours if infusion-related reactions occur).",
                "html": "<p>For <i>intravenous infusion</i> (<i>Soliris</i>\n<tm tmtype=\"reg\"/>), give intermittently <i>in</i> Glucose 5% <i>or</i> Sodium chloride 0.9%. Dilute requisite dose to a concentration of 5&#8239;mg/mL and mix gently; give over 25&#8211;45 minutes (infusion time may be increased to 2 hours if infusion-related reactions occur).</p>"
            }
        ]
    },
