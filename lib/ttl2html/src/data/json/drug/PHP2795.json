    "directionsForAdministration": {
        "directionsForAdministration": [
            {
                "type": "directionsForAdministration",
                "textContent": "Tramadol hydrochloride orodispersible tablets should be sucked and then swallowed. May also be dispersed in water.\n\nSome tramadol hydrochloride modified-release capsule preparations may be opened and the content swallowed immediately without chewing&#8212;check individual preparations.",
                "html": "<p>Tramadol hydrochloride <i>orodispersible tablets</i> should be sucked and then swallowed. May also be dispersed in water.</p><p>Some tramadol hydrochloride modified-release capsule preparations may be opened and the content swallowed immediately without chewing&#8212;check individual preparations.</p>"
            },
            {
                "type": "directionsForAdministration",
                "textContent": "For intravenous infusion, dilute in Glucose 5% or Sodium Chloride 0.9%.",
                "html": "<p>For <i>intravenous infusion</i>, dilute in Glucose 5% or Sodium Chloride 0.9%.</p>"
            }
        ]
    },
    "prescribingAndDispensingInformation": {
        "prescribingAndDispensingInformation": [
            {
                "type": "prescribingAndDispensingInformation",
                "textContent": "Do not confuse modified-release 12-hourly preparations with 24-hourly preparations.",
                "html": "<p>Do not confuse modified-release 12-hourly preparations with 24-hourly preparations.</p>"
            }
        ]
    },
