    "pregnancy": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Avoid at term&#8212;may produce neonatal haemolysis.",
                "html": "<p>Avoid at term&#8212;may produce neonatal haemolysis.</p>"
            }
        ]
    },
    "breastFeeding": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Avoid; only small amounts in milk but enough to produce haemolysis in G6PD-deficient infants.",
                "html": "<p>Avoid; only small amounts in milk but enough to produce haemolysis in G6PD-deficient infants.</p>"
            }
        ]
    },
    "hepaticImpairment": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Use with caution; cholestatic jaundice and chronic active hepatitis reported.",
                "html": "<p>Use with caution; cholestatic jaundice and chronic active hepatitis reported.</p>"
            }
        ]
    },
    "renalImpairment": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Risk of peripheral neuropathy; antibacterial efficacy depends on renal secretion of the drug into urinary tract.",
                "html": "<p>Risk of peripheral neuropathy; antibacterial efficacy depends on renal secretion of the drug into urinary tract.</p>"
            },
            {
                "type": "generalInformation",
                "specificity": {
                    "textContent": "In adults",
                    "html": "In adults",
                    "patientType": "adults"
                },
                "textContent": "Avoid if eGFR less than 45 mL/ minute/1.73 m2; may be used with caution if eGFR 30&#8211;44 mL/ minute/1.73 m2 as a short-course only (3 to 7 days), to treat uncomplicated lower urinary-tract infection caused by suspected or proven multidrug resistant bacteria and only if potential benefit outweighs risk.",
                "html": "<p>Avoid if eGFR less than 45&#8239;mL/ minute/1.73&#8239;m<sup>2</sup>; may be used with caution if eGFR 30&#8211;44&#8239;mL/ minute/1.73&#8239;m<sup>2</sup> as a short-course only (3 to 7 days), to treat uncomplicated lower urinary-tract infection caused by suspected or proven multidrug resistant bacteria and only if potential benefit outweighs risk.</p>"
            },
            {
                "type": "generalInformation",
                "specificity": {
                    "textContent": "In children",
                    "html": "In children",
                    "patientType": "children"
                },
                "textContent": "Avoid if estimated glomerular filtration rate less than 45 mL/minute/1.73 m 2; may be used with caution if estimated glomerular filtration rate 30&#8211;44 mL/ minute/1.73 m2 as a short-course only (3 to 7 days), to treat uncomplicated lower urinary-tract infection caused by suspected or proven multidrug resistant bacteria and only if potential benefit outweighs risk.",
                "html": "<p>Avoid if estimated glomerular filtration rate less than 45&#8239;mL/minute/1.73&#8239;m <sup>2</sup>; may be used with caution if estimated glomerular filtration rate 30&#8211;44&#8239;mL/ minute/1.73&#8239;m<sup>2</sup> as a short-course only (3 to 7 days), to treat uncomplicated lower urinary-tract infection caused by suspected or proven multidrug resistant bacteria and only if potential benefit outweighs risk.</p>"
            }
        ]
    },
    "monitoringRequirements": {
        "patientParameters": [
            {
                "type": "patientParameters",
                "specificity": {
                    "textContent": "In adults",
                    "html": "In adults",
                    "patientType": "adults"
                },
                "textContent": "On long-term therapy, monitor liver function and monitor for pulmonary symptoms, especially in the elderly (discontinue if deterioration in lung function).",
                "html": "<p>On long-term therapy, monitor liver function and monitor for pulmonary symptoms, especially in the elderly (discontinue if deterioration in lung function).</p>"
            },
            {
                "type": "patientParameters",
                "specificity": {
                    "textContent": "In children",
                    "html": "In children",
                    "patientType": "children"
                },
                "textContent": "On long-term therapy, monitor liver function and monitor for pulmonary symptoms (discontinue if deterioration in lung function).",
                "html": "<p>On long-term therapy, monitor liver function and monitor for pulmonary symptoms (discontinue if deterioration in lung function).</p>"
            }
        ]
    },
