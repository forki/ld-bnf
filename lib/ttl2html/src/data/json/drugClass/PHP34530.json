        "advice": [
            {
                "type": "advice",
                "title": "Hepatic effects",
                "textContent": "In light of reports of cholestatic jaundice, hepatitis, fulminant hepatic necrosis, and hepatic failure, ACE inhibitors should be discontinued if marked elevation of hepatic enzymes or jaundice occur.",
                "html": "<p>In light of reports of cholestatic jaundice, hepatitis, fulminant hepatic necrosis, and hepatic failure, ACE inhibitors should be discontinued if marked elevation of hepatic enzymes or jaundice occur.</p>"
            }
        ]
    },
    "allergyAndCrossSensitivity": {
        "contraindications": [
            {
                "type": "contraindications",
                "textContent": "ACE inhibitors are contra-indicated in patients with hypersensitivity to ACE inhibitors (including angioedema).",
                "html": "<p>ACE inhibitors are contra-indicated in patients with hypersensitivity to ACE inhibitors (including angioedema).</p>"
            }
        ]
    },
    "pregnancy": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "ACE inhibitors should be avoided in pregnancy unless essential. They may adversely affect fetal and neonatal blood pressure control and renal function; skull defects and oligohydramnios have also been reported.",
                "html": "<p>ACE inhibitors should be avoided in pregnancy unless essential. They may adversely affect fetal and neonatal blood pressure control and renal function; skull defects and oligohydramnios have also been reported.</p>"
            }
        ]
    },
    "breastFeeding": {
        "generalInformation": [
            {
                "type": "generalInformation",
                "textContent": "Information on the use of ACE inhibitors in breast-feeding is limited.",
                "html": "<p>Information on the use of ACE inhibitors in breast-feeding is limited.</p>"
            }
        ]
    },
    "renalImpairment": {
        "doseAdjustments": [
            {
                "type": "doseAdjustments",
                "textContent": "Use with caution, starting with low dose, and adjust according to response. Hyperkalaemia and other side-effects of ACE inhibitors are more common in those with impaired renal function and the dose may need to be reduced.",
                "html": "<p>Use with caution, starting with low dose, and adjust according to response. Hyperkalaemia and other side-effects of ACE inhibitors are more common in those with impaired renal function and the dose may need to be reduced.</p>"
            }
        ]
    },
    "monitoringRequirements": {
        "patientParameters": [
            {
                "type": "patientParameters",
                "textContent": "Renal function and electrolytes should be checked before starting ACE inhibitors (or increasing the dose) and monitored during treatment (more frequently if side effects mentioned are present).",
                "html": "<p>Renal function and electrolytes should be checked before starting ACE inhibitors (or increasing the dose) and monitored during treatment (more frequently if side effects mentioned are present).</p>"
            }
        ]
    },
    "directionsForAdministration": {
        "directionsForAdministration": [
            {
                "type": "directionsForAdministration",
                "textContent": "For hypertension the first dose should preferably be given at bedtime.",
                "html": "<p>For hypertension the first dose should preferably be given at bedtime.</p>"
            }
        ]
    },
