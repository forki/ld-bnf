    "xml": "<topic id=\"PHP6089\" outputclass=\"drugClass\" rev=\"1.16\" type=\"drugClass\" namespace=\"/drug-classes/calcium-salts\" basename=\"calcium-salts\" title=\"CALCIUM SALTS\">\n<title>CALCIUM SALTS</title>\n<topic id=\"PHP55361\" outputclass=\"contraindications\" rev=\"1.14\" parent=\"/drug-classes/calcium-salts\">\n<title>Contra-indications</title>\n<body>\n<section outputclass=\"contraindications\">\n<p>\n<ph outputclass=\"contraindication\">Conditions associated with hypercalcaemia (e.g. some forms of malignant disease)</ph>; <ph outputclass=\"contraindication\">conditions associated with hypercalciuria (e.g. some forms of malignant disease)</ph>\n</p>\n</section>\n</body>\n</topic>\n<topic id=\"PHP55371\" outputclass=\"cautions\" rev=\"1.7\" parent=\"/drug-classes/calcium-salts\">\n<title>Cautions</title>\n<body>\n<section outputclass=\"cautions\">\n<p>\n<ph outputclass=\"caution\">History of nephrolithiasis</ph>; <ph outputclass=\"caution\">sarcoidosis</ph>\n</p>\n</section>\n</body>\n</topic>\n<topic id=\"PHP55290\" outputclass=\"interactions\" rev=\"1.7\" parent=\"/drug-classes/calcium-salts\">\n<title>Interactions</title>\n<body>\n<section outputclass=\"general\">\n<sectiondiv>\n<p>Appendix 1 (antacids, calcium salts).</p>\n</sectiondiv>\n</section>\n</body>\n</topic>\n<topic id=\"PHP55220\" outputclass=\"sideEffects\" rev=\"1.22\" parent=\"/drug-classes/calcium-salts\">\n<title>Side-effects</title>\n<body>\n<section outputclass=\"generalSideEffects\">\n<title>General side-effects:</title>\n<sectiondiv outputclass=\"frequencies\">\n<sectiondiv outputclass=\"rare\">\n<p outputclass=\"title\">Rare</p>\n<p outputclass=\"sideEffects\">\n<ph outputclass=\"sideEffect\">Gastro-intestinal disturbances</ph>\n</p>\n</sectiondiv>\n<sectiondiv outputclass=\"notKnown\">\n<p outputclass=\"title\">Frequency not known</p>\n<p outputclass=\"sideEffects\">\n<ph outputclass=\"sideEffect\">Hypercalcaemia</ph>\n</p>\n</sectiondiv>\n</sectiondiv>\n</section>\n<section outputclass=\"specificSideEffects\">\n<title>Specific side-effects:</title>\n<sectiondiv outputclass=\"frequencies\">\n<sectiondiv outputclass=\"notKnown\">\n<p outputclass=\"title\">Frequency not known</p>\n<sectiondiv outputclass=\"sideEffectsWithRoutes\">\n<p outputclass=\"title\">With intravenous use</p>\n<p>\n<ph outputclass=\"sideEffect\">arrhythmias</ph>; <ph outputclass=\"sideEffect\">bradycardia</ph>; <ph outputclass=\"sideEffect\">fall in blood pressure</ph>; <ph outputclass=\"sideEffect\">injection-site reactions</ph>; <ph outputclass=\"sideEffect\">peripheral vasodilatation</ph>; <ph outputclass=\"sideEffect\">severe tissue damage with extravasation</ph>; <ph outputclass=\"sideEffect\">sweating</ph>\n</p>\n</sectiondiv>\n</sectiondiv>\n</sectiondiv>\n</section>\n</body>\n</topic>\n<topic id=\"PHP55255\" outputclass=\"renalImpairment\" parent=\"/drug-classes/calcium-salts\">\n<title>Renal impairment</title>\n<body>\n<section outputclass=\"generalInformation\">\n<sectiondiv>\n<p>Use with caution.</p>\n</sectiondiv>\n</section>\n</body>\n</topic>\n<parents>\n<xref format=\"dita\" scope=\"local\" type=\"drugClasses\" href=\"#drugClasses\" title=\"Drug classes\" namespace=\"/drug-classes\">Drug classes</xref>\n</parents>\n<drugs>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP104228\" title=\"CALCIUM ACETATE\" namespace=\"/drugs/calcium-acetate\">CALCIUM ACETATE</xref>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP93600\" title=\"CALCIUM GLUCONATE\" namespace=\"/drugs/calcium-gluconate\">CALCIUM GLUCONATE</xref>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP93601\" title=\"CALCIUM LACTATE\" namespace=\"/drugs/calcium-lactate\">CALCIUM LACTATE</xref>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP93602\" title=\"CALCIUM CARBONATE\" namespace=\"/drugs/calcium-carbonate\">CALCIUM CARBONATE</xref>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP93603\" title=\"CALCIUM CHLORIDE\" namespace=\"/drugs/calcium-chloride\">CALCIUM CHLORIDE</xref>\n</drugs>\n<backlinks/>\n<links>\n<xref format=\"dita\" scope=\"local\" type=\"#drugClasses\" href=\"#drugClasses\" namespace=\"/drug-classes\" title=\"Drug classes\" count=\"1\" rel=\"link\">Drug classes</xref>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP104228\" namespace=\"/drugs/calcium-acetate\" title=\"CALCIUM ACETATE\" count=\"1\" rel=\"link\">CALCIUM ACETATE</xref>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP93600\" namespace=\"/drugs/calcium-gluconate\" title=\"CALCIUM GLUCONATE\" count=\"1\" rel=\"link\">CALCIUM GLUCONATE</xref>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP93601\" namespace=\"/drugs/calcium-lactate\" title=\"CALCIUM LACTATE\" count=\"1\" rel=\"link\">CALCIUM LACTATE</xref>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP93602\" namespace=\"/drugs/calcium-carbonate\" title=\"CALCIUM CARBONATE\" count=\"1\" rel=\"link\">CALCIUM CARBONATE</xref>\n<xref format=\"dita\" scope=\"local\" type=\"drug\" href=\"#PHP93603\" namespace=\"/drugs/calcium-chloride\" title=\"CALCIUM CHLORIDE\" count=\"1\" rel=\"link\">CALCIUM CHLORIDE</xref>\n</links>\n</topic>",
