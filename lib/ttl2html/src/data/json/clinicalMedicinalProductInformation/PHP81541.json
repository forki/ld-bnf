    "xml": "<topic id=\"PHP81541\" outputclass=\"clinicalMedicinalProductInformation\" type=\"clinicalMedicinalProductInformation\" namespace=\"/clinical-medicinal-product-information/subgam\" basename=\"subgam\" title=\"Subgam\">\n<title>Subgam<tm tmtype=\"reg\"/>\n</title>\n<topic id=\"PHP82330\" outputclass=\"indicationsAndDose\" rev=\"1.16\" parent=\"/clinical-medicinal-product-information/subgam\">\n<title>Indications and dose</title>\n<body>\n<section outputclass=\"indicationAndDoseGroup\">\n<sectiondiv outputclass=\"therapeuticIndications\">\n<p outputclass=\"therapeuticIndication\">Hepatitis A prophylaxis in outbreaks</p>\n</sectiondiv>\n<p outputclass=\"routesOfAdministration\">By intramuscular injection</p>\n<ul outputclass=\"patientGroups\">\n<li outputclass=\"patientGroup adult\">\n<p>Adult</p>\n<p>750&#8239;mg.</p>\n</li>\n</ul>\n</section>\n</body>\n</topic>\n<topic id=\"PHP82337\" outputclass=\"unlicensedUse\" rev=\"1.20\" parent=\"/clinical-medicinal-product-information/subgam\">\n<title>Unlicensed use</title>\n<body>\n<section>\n<sectiondiv>\n<p>\n<i>Subgam</i>\n<tm tmtype=\"reg\"/> is not licensed for prophylactic use, but due to difficulty in obtaining suitable immunoglobulin products, Public Health England recommends intramuscular use for prophylaxis against Hepatitis A or rubella.</p>\n</sectiondiv>\n</section>\n</body>\n</topic>\n<parents>\n<xref format=\"dita\" scope=\"local\" type=\"clinicalMedicinalProductInformation\" href=\"#clinicalMedicinalProductInformation\" title=\"Clinical medicinal product information\" namespace=\"/clinical-medicinal-product-information\">Clinical medicinal product information</xref>\n</parents>\n<backlinks/>\n<links>\n<xref format=\"dita\" scope=\"local\" type=\"#clinicalMedicinalProductInformation\" href=\"#clinicalMedicinalProductInformation\" namespace=\"/clinical-medicinal-product-information\" title=\"Clinical medicinal product information\" count=\"1\" rel=\"link\">Clinical medicinal product information</xref>\n</links>\n</topic>",
