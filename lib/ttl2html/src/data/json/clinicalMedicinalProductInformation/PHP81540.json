    "xml": "<topic id=\"PHP81540\" outputclass=\"clinicalMedicinalProductInformation\" type=\"clinicalMedicinalProductInformation\" namespace=\"/clinical-medicinal-product-information/zostavax\" basename=\"zostavax\" title=\"Zostavax\">\n<title>Zostavax<tm tmtype=\"reg\"/>\n</title>\n<topic id=\"PHP82266\" outputclass=\"indicationsAndDose\" rev=\"1.21\" parent=\"/clinical-medicinal-product-information/zostavax\">\n<title>Indications and dose</title>\n<body>\n<section outputclass=\"indicationAndDoseGroup\">\n<sectiondiv outputclass=\"therapeuticIndications\">\n<p outputclass=\"therapeuticIndication\">Prevention of herpes zoster (shingles)</p>\n</sectiondiv>\n<p outputclass=\"routesOfAdministration\">By subcutaneous injection</p>\n<ul outputclass=\"patientGroups\">\n<li outputclass=\"patientGroup adult\">\n<p>Adult 70&#8211;79 years</p>\n<p>0.65&#8239;mL for 1 dose, to be administered preferably into the deltoid region.</p>\n</li>\n</ul>\n</section>\n</body>\n</topic>\n<topic id=\"PHP82301\" outputclass=\"prescribingAndDispensingInformation\" rev=\"1.10\" parent=\"/clinical-medicinal-product-information/zostavax\">\n<title>Prescribing and dispensing information</title>\n<body>\n<section>\n<sectiondiv>\n<p>Advice in the BNF may differ from that in product literature.</p>\n</sectiondiv>\n</section>\n</body>\n</topic>\n<parents>\n<xref format=\"dita\" scope=\"local\" type=\"clinicalMedicinalProductInformation\" href=\"#clinicalMedicinalProductInformation\" title=\"Clinical medicinal product information\" namespace=\"/clinical-medicinal-product-information\">Clinical medicinal product information</xref>\n</parents>\n<backlinks/>\n<links>\n<xref format=\"dita\" scope=\"local\" type=\"#clinicalMedicinalProductInformation\" href=\"#clinicalMedicinalProductInformation\" namespace=\"/clinical-medicinal-product-information\" title=\"Clinical medicinal product information\" count=\"1\" rel=\"link\">Clinical medicinal product information</xref>\n</links>\n</topic>",
