    "xml": "<topic id=\"PHP78976\" outputclass=\"clinicalMedicinalProductInformation\" type=\"clinicalMedicinalProductInformation\" namespace=\"/clinical-medicinal-product-information/gonapeptyl-depot\" basename=\"gonapeptyl-depot\" title=\"Gonapeptyl Depot\">\n<title>Gonapeptyl Depot<tm tmtype=\"reg\"/>\n</title>\n<topic id=\"PHP79161\" outputclass=\"indicationsAndDose\" rev=\"1.42\" parent=\"/clinical-medicinal-product-information/gonapeptyl-depot\">\n<title>Indications and dose</title>\n<body>\n<section outputclass=\"indicationAndDoseGroup\">\n<sectiondiv outputclass=\"therapeuticIndications\">\n<p outputclass=\"therapeuticIndication\">Advanced prostate cancer</p>\n</sectiondiv>\n<p outputclass=\"routesOfAdministration\">By subcutaneous injection or by deep intramuscular injection</p>\n<ul outputclass=\"patientGroups\">\n<li outputclass=\"patientGroup adult\">\n<p>Adult</p>\n<p>3.75&#8239;mg every 4&#8239;weeks.</p>\n</li>\n</ul>\n</section>\n<section outputclass=\"indicationAndDoseGroup\">\n<sectiondiv outputclass=\"therapeuticIndications\">\n<p outputclass=\"therapeuticIndication\">Endometriosis</p>\n<p outputclass=\"therapeuticIndication\">Reduction in size of fibroids</p>\n</sectiondiv>\n<p outputclass=\"routesOfAdministration\">By subcutaneous injection or by deep intramuscular injection</p>\n<ul outputclass=\"patientGroups\">\n<li outputclass=\"patientGroup adult\">\n<p>Adult</p>\n<p>3.75&#8239;mg every 4&#8239;weeks maximum duration of 6 months (not to be repeated), to be started during first 5 days of menstrual cycle.</p>\n</li>\n</ul>\n</section>\n</body>\n</topic>\n<parents>\n<xref format=\"dita\" scope=\"local\" type=\"clinicalMedicinalProductInformation\" href=\"#clinicalMedicinalProductInformation\" title=\"Clinical medicinal product information\" namespace=\"/clinical-medicinal-product-information\">Clinical medicinal product information</xref>\n</parents>\n<backlinks/>\n<links>\n<xref format=\"dita\" scope=\"local\" type=\"#clinicalMedicinalProduct