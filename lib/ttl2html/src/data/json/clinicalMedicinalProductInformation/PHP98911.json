    "xml": "<topic id=\"PHP98911\" outputclass=\"clinicalMedicinalProductInformation\" type=\"clinicalMedicinalProductInformation\" namespace=\"/clinical-medicinal-product-information/emerade-500-micrograms\" basename=\"emerade-500-micrograms\" title=\"Emerade 500 micrograms\">\n<title>Emerade<tm tmtype=\"reg\"/> 500 micrograms</title>\n<topic id=\"PHP98938\" outputclass=\"indicationsAndDose\" rev=\"1.12\" parent=\"/clinical-medicinal-product-information/emerade-500-micrograms\">\n<title>Indications and dose</title>\n<body>\n<section outputclass=\"indicationAndDoseGroup\">\n<sectiondiv outputclass=\"therapeuticIndications\">\n<p outputclass=\"therapeuticIndication\">Acute anaphylaxis (for self-administration for patients at risk of severe anaphylaxis)</p>\n</sectiondiv>\n<p outputclass=\"routesOfAdministration\">By intramuscular injection</p>\n<ul outputclass=\"patientGroups\">\n<li outputclass=\"patientGroup child\">\n<p>Child 12&#8211;17 years</p>\n<p>500&#8239;micrograms, then 500&#8239;micrograms after 5&#8211;15&#8239;minutes as required.</p>\n</li>\n<li outputclass=\"patientGroup adult\">\n<p>Adult</p>\n<p>500&#8239;micrograms, then 500&#8239;micrograms after 5&#8211;15&#8239;minutes as required.</p>\n</li>\n</ul>\n</section>\n</body>\n</topic>\n<topic id=\"PHP99020\" outputclass=\"patientAndCarerAdvice\" rev=\"1.9\" parent=\"/clinical-medicinal-product-information/emerade-500-micrograms\">\n<title>Patient and carer advice</title>\n<body>\n<section outputclass=\"generalPatientAdvice\">\n<sectiondiv>\n<p>No solution remains in the auto-injector device after use.</p>\n</sectiondiv>\n</section>\n</body>\n</topic>\n<parents>\n<xref format=\"dita\" scope=\"local\" type=\"clinicalMedicinalProductInformation\" href=\"#clinicalMedicinalProductInformation\" title=\"Clinical medicinal product information\" namespace=\"/clinical-medicinal-product-information\">Clinical medicinal product information</xref>\n</parents>\n<backlinks/>\n<links>\n<xref format=\"dita\" scope=\"